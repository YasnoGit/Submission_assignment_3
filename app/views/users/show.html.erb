<div>
  <h2>User info</h2>
  <%= image_tag @user.get_profile_image(100,100) %>
  <table>
    <tbody>
      <tr>
      <% @users.each do |user| %>
      </tr>
      <tr>
        <th>name</th>
        <th><%= name %></th>
      </tr>
      <tr>
        <th>introduction</th>
        <th><%= introduction %></th>
      </tr>
    </tbody>
  </table>
</div>

